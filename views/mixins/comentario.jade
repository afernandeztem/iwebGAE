mixin comentario(comentario,intrusion)
   .material-card
    .card-body
       - const id = comentario.id
       h5.card-title #{comentario.titulo}
       h7.card-text #{comentario.usuario}
       p.card-text #{comentario.contenido}
       if !intrusion
            a(href='/comentarios/edit?id=' + id ) Editar
            a(href='/comentarios/eliminar?id=' + id ) Eliminar
       br
       br
